{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "<#42E66C>\u2b22 </><b>{{ .Path }}</b>",
          "foreground": "#F1FA8C",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#ffffff> âš¡ </><b>{{ .HEAD }}</b>",
          "foreground": "#43CCEA",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ff0000> \u25c9</>",
          "foreground": "#ff0000",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": ">",
          "foreground": "#000000",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
